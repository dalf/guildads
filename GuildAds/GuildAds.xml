<Ui xmlns="http://www.blizzard.com/wow/ui/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/FrameXML/UI.xsd">
	
	<Frame name="GuildAdsSystem" parent="UIParent">
		<Scripts>
			<OnUpdate>
				GuildAdsTask:Update();
				GuildAdsComm:ProcessQueues(arg1);
			</OnUpdate>
		</Scripts>
	</Frame>
	
	<GameTooltip name="GuildAdsITT" inherits="GameTooltipTemplate" parent="UIParent" frameStrata="HIGH" hidden="true">
		<Size>
			<AbsDimension x="128" y="64"/>
		</Size>
		<Scripts>
			<OnShow>
				GuildAdsInternalTooltip_ItemReady();
			</OnShow>
		</Scripts>
	</GameTooltip>
	
</Ui>
